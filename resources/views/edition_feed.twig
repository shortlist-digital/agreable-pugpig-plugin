<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xmlns:app="http://www.w3.org/2007/app" xmlns:dcterms="http://purl.org/dc/terms/">
  <id>editions_{{feed.edition_key}}</id>
  <!-- Atom Contents Feeds - Generated: Wed, 28 Oct 15 15:55:38 +0000 -->
  <link type="application/atom+xml" href="{{feed.atom_url}}" rel="self"/>
  <title>{{feed.edition_title}}</title>
  <updated>{{feed.last_updated}}</updated>
  <author>
    <name>Shortlist</name>
  </author>
  {% for post in posts %}
  <entry>
    <title>{{post.post_title}}</title>
    <id>page-{{post.id}}</id>
    <updated>{{post.last_updated}}</updated>
    <published>{{post.published_time}}</published>
    <author>
      <name>{{feed.site_name}}</name>
    </author>
    <summary type="text"></summary>
    <category scheme="http://schema.pugpig.com/section" term="{{post.section}}"/>
    <category scheme="http://schema.pugpig.com/access" term=""/>
    <category scheme="http://schema.pugpig.com/pagetype" term="post"/>
    <category scheme="http://schema.pugpig.com/level" term="1"/>
    <link type="text/cache-manifest" href="{{post.manifest}}" rel="related"/>
    <link type="text/html" href="{{post.index}}" rel="alternate"/>
    <link type="text/html" href="{{post.permalink}}" rel="bookmark"/>
  </entry>
  {% endfor %}
</feed>
